<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Login Test" >
    <test name="Login Test">
        <groups>
            <run>
                <include name="goToWebSiteAndChangeLangToEnglish"></include>
            </run>
        </groups>
        <classes>
            <class name="logIn.LoginTest">
                <methods>
                    <include name="logInSucces"></include>
                    <include name="logInUnsucces"></include>
                    <include name="forgotPassword"></include>
                    <include name="logoutFun"></include>
                </methods>
            </class>
        </classes>
    </test>
</suite>